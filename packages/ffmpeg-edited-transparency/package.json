{
  "name": "@motion-canvas/ffmpeg-edited-transparency",
  "version": "1.2.1",
  "private":true,
  "description": "An edited version of the FFmpeg video exporter for Motion Canvas so it supports transparency",
  "main": "lib/server/index.js",
  "author": "motion-canvas, S1rDev10us",
  "homepage": "https://motioncanvas.io/",
  "bugs": "https://github.com/motion-canvas/exporters/issues",
  "license": "MIT",
  "scripts": {
    "build": "npm run client:build && npm run server:build",
    "client:build": "tsc --project client/tsconfig.json",
    "client:dev": "tsc -w --project client/tsconfig.json",
    "server:build": "tsc --project server/tsconfig.json",
    "server:dev": "tsc -w --project server/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/S1rDev10us/Typescript-MotionCanvasExporters.git"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "@motion-canvas/core": "^3.8.0",
    "@motion-canvas/vite-plugin": "^3.7.0",
    "vite": "4.x"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.21"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.0.0",
    "fluent-ffmpeg": "^2.1.2"
  }
}
